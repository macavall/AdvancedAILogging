{
  "version": "2.0",
  "logging": {
    "console": {
      "isEnabled": true
    },
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Trace",
      "Host": "Trace",
      "Host.ScaleMonitor": "Trace",
      "Function": "Trace",
      "Microsoft.Azure.WebJobs.ServiceBus": "Trace",
      "Microsoft.Azure.Functions.Worker": "Trace"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "scaleController": {
        "enabled": true
      }
    }
  },
  "extensions": {
    "serviceBus": {
      "prefetchCount": 100,
      "maxConcurrentCalls": 16,
      "autoCompleteMessages": true
    }
  }
}